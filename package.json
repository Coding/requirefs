{
  "name": "@coder/requirefs",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Coder",
  "description": "Create a readable and requirable file system from tars, zips, or a custom provider.",
  "main": "./out/requirefs",
  "scripts": {
    "test": "mocha -r ts-node/register ./test/*.test.ts",
    "lint": "eslint {src,test}/*.ts",
    "generateZip": "rm -f ./test/lib.zip && cd ./test/lib && zip -r ../lib.zip ./*",
    "generateTar": "rm -f ./test/lib.tar && tar -cf ./test/lib.tar -C ./test/lib .",
    "prepare": "tsc"
  },
  "devDependencies": {
    "@types/jszip": "0.0.21-alpha",
    "@types/mocha": "2.2.39",
    "@types/tar-fs": "^1.16.1",
    "@types/tar-stream": "^1.6.1",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^2.2.5",
    "prettier": "^1.18.2",
    "tar-fs": "^2.0.0",
    "text-encoding": "^0.7.0",
    "ts-node": "^8.3.0",
    "typescript": "~3.5.0"
  },
  "dependencies": {
    "jszip": "2.6.0",
    "tar-stream": "^2.1.0"
  }
}
